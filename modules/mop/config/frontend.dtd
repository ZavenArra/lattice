<!ELEMENT data (view*) >

<!ELEMENT view (extendeddata|subview)* >
<!ATTLIST view
	name				CDATA #REQUIRED
	loadPage			(true|false) "true"
>


<!ELEMENT extendeddata >
<!ATTLIST extendeddata
	label				CDATA #REQUIRED
	templatename		CDATA #IMPLIED
	parent		 		(CDATA|parent) "parent"
>

<!ELEMENT subview >
<!ATTLIST subvjew
	view				CDATA #REQUIRED
	label				CDATA #REQUIRED
>
