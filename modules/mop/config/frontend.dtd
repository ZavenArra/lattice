<!ELEMENT data (view*) >

<!ELEMENT view (extendeddata|subview)* >
<!ATTLIST view
	name				CDATA #REQUIRED
	loadpage			(true|false) "true"
>


<!ELEMENT extendeddata EMPTY >
<!ATTLIST extendeddata
	label				CDATA #REQUIRED
	templatename		CDATA #IMPLIED
	parent		 		CDATA "parent"
>

<!ELEMENT subview EMPTY >
<!ATTLIST subvjew
	view				CDATA #REQUIRED
	label				CDATA #REQUIRED
>
